{
  "name": "asseco-voice/laravel-eloquent-event-broadcaster",
  "description": "Laravel eloquent events broadcasting",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "asseco-voice/laravel-stomp": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.4.4",
    "fakerphp/faker": "^1.9.1",
    "orchestra/testbench": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Asseco\\EloquentEventBroadcaster\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Asseco\\EloquentEventBroadcaster\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Asseco\\EloquentEventBroadcaster\\BroadcasterServiceProvider"
      ]
    }
  }
}
